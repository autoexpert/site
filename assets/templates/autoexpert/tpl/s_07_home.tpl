
<!-- time is money-->
<section class="section timemoney gray">
	<div data-wow-duration="1s" style="visibility: hidden;" class="wrapper-page no-animation wow fadeInUp">
		<div class="stock-block">
			<div class="stock-block-left"></div>
			<div class="stock-block-right">
				<div class="stock-block-heading">
					<h3>Время - деньги!</h3>
